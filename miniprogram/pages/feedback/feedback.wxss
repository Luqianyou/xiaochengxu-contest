/* pages/feedback/feedback.wxss */
page {
  width: 100%;
  height: 100%;
  position: relative;
  background: #fff;
}
.feedback_top{
  position: absolute;
  display: flex;
  flex: 1;
  top: 10rpx;

}
.feedback_text{
  font-size: 40rpx;
}
.feedback_text view{
  margin: 10rpx;
  padding: 10rpx;
  border: 10rpx solid #ccc;
border-radius: 20rpx;
}
.feedback_top image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.g-container {
  position: absolute;
  width: 200px;
  height: 200px;
  margin: auto;
  top: 45%;
  left: 25%;
  
  border-radius: 50%;
  box-sizing: border-box;
  border: 6px solid #444;
}
.g-circle {
  position: absolute;
  top: -6px;
  left: -6px;
  bottom: -6px;
  right: -6px;
  border-radius: 50%;
  background: conic-gradient(#fc0 0%, transparent 40%, transparent);
  animation: rotate 3s ease-in-out infinite;
  /* transition-origin: 50% 50%; */
}

@keyframes rotate {
  100% {
      transform: rotate(-360deg);
  }
}
button{
  background-color: #99c;
}