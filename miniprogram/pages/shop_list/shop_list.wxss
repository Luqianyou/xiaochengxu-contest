page{
  background-color:#F4F4F4;
}
.shoplisttop{
width: 100%;
}

.shoplisttop image{
  width: 100%;
}

.content{
  margin:0 20rpx;
}

.shopping{
  border-radius: 10rpx;
  padding: 10rpx 10rpx;
  box-sizing: border-box;
  background-color: #fff;
  position: relative;
  height: 200rpx;
  margin-bottom: 20rpx;
}

.title{
  font-weight: bold;
}

.shopping .price{
  color: red;
  position: absolute;
  left: 10rpx;
  bottom: 10rpx;
}

.shopping .shoptop{
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  color: white;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-family: cursive;
  background-color: #9999ff;
}

.sh_describe{
  border-radius: 10rpx;
  background-color: #fff;
}

.shopcar-view{
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
}

.shopcar-view view{
  border-radius: 50rpx;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: white;
  background-color: #9999ff;
  margin-bottom: -20rpx;
  font-size: 24rpx;
}

.shopcar-view image{
  width: 110rpx;
  height: 110rpx;
}